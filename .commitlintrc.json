{
  "extends": [
    "@commitlint/config-conventional"
  ],
  "rules": {
		"type-enum": [
			2,
			"always",
			[
				"build",
				"ci",
				"docs",
				"feat",
        "update",
        "del",
        "merge",
				"fix",
				"perf",
				"refactor",
				"style",
        "test",
				"revert",
				"chore"
			]
		]
	}
}