{
  "name": "jzs-admin",
  "version": "1.0.0",
  "description": "中后台管理系统",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=local node server.js --max_old_space_size=4096",
    "start-dev": "cross-env NODE_ENV=dev node server.js --max_old_space_size=4096",
    "start-test": "cross-env NODE_ENV=test node server.js --max_old_space_size=4096",
    "start-pro": "cross-env NODE_ENV=prod node server.js --max_old_space_size=4096",
    "dist": "cross-env NODE_ENV=prod node server.js",
    "dev": "cross-env NODE_ENV=dev webpack --config webpack.build.js",
    "test": "cross-env NODE_ENV=test webpack --config webpack.build.js",
    "build": "cross-env NODE_ENV=prod webpack --config webpack.build.js",
    "dll": "webpack --config webpack.dll.js",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "jest": "jest"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "JZS",
  "license": "ISC",
  "private": true,
  "installConfig": {
    "pnp": false
  },
